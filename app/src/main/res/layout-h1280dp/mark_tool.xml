<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:nendsdk="http://schemas.android.com/apk/res-auto"
	android:id="@+id/markToolScrollLayout"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<android.support.constraint.ConstraintLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<TextView
			android:id="@+id/textView3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="16dp"
			android:text="@string/agari"
			android:textColor="?attr/colorAccent"
			android:textSize="20sp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<Spinner
			android:id="@+id/spinner"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="8dp"
			android:entries="@array/list3"
			android:spinnerMode="dropdown"
			app:layout_constraintStart_toStartOf="@+id/textView3"
			app:layout_constraintTop_toBottomOf="@+id/textView3" />

		<View
			android:id="@+id/view5"
			android:layout_width="match_parent"
			android:layout_height="5dp"
			android:layout_marginStart="8dp"
			android:layout_marginTop="16dp"
			android:layout_marginEnd="8dp"
			android:background="?attr/colorPrimaryDark"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/spinner" />

		<TextView
			android:id="@+id/textView7"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="16dp"
			android:text="@string/head"
			android:textColor="?attr/colorAccent"
			android:textSize="20sp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/view5" />

		<Spinner
			android:id="@+id/spinner2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="8dp"
			android:entries="@array/list4"
			android:spinnerMode="dropdown"
			app:layout_constraintStart_toStartOf="@+id/textView7"
			app:layout_constraintTop_toBottomOf="@+id/textView7" />

		<View
			android:id="@+id/view6"
			android:layout_width="match_parent"
			android:layout_height="5dp"
			android:layout_marginStart="8dp"
			android:layout_marginTop="16dp"
			android:layout_marginEnd="8dp"
			android:background="?attr/colorPrimaryDark"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/spinner2" />

		<TextView
			android:id="@+id/textView9"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="16dp"
			android:text="@string/machi"
			android:textColor="?attr/colorAccent"
			android:textSize="20sp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/view6" />

		<Spinner
			android:id="@+id/spinner3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="8dp"
			android:entries="@array/list5"
			android:spinnerMode="dropdown"
			app:layout_constraintStart_toStartOf="@+id/textView7"
			app:layout_constraintTop_toBottomOf="@+id/textView9" />

		<View
			android:id="@+id/view7"
			android:layout_width="match_parent"
			android:layout_height="5dp"
			android:layout_marginStart="8dp"
			android:layout_marginTop="16dp"
			android:layout_marginEnd="8dp"
			android:background="?attr/colorPrimaryDark"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/spinner3" />

		<TextView
			android:id="@+id/textView16"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="16dp"
			android:text="@string/mentu"
			android:textColor="?attr/colorAccent"
			android:textSize="20sp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/view7" />

		<android.support.v7.widget.GridLayout
			android:id="@+id/gridLayout"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginTop="8dp"
			app:layout_constraintStart_toStartOf="@+id/textView16"
			app:layout_constraintTop_toBottomOf="@+id/textView16">

			<!-- 1行目 -->
			<TextView
				android:id="@+id/textView17"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/blank_table"
				android:textSize="18sp"
				app:layout_gravity="center" />

			<TextView
				android:id="@+id/tyutyanTextView"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/tyutyan"
				android:textSize="18sp"
				app:layout_column="2"
				app:layout_gravity="center"
				app:layout_row="0" />

			<TextView
				android:id="@+id/kokyuTextView"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/tanyao"
				android:textSize="18sp"
				app:layout_column="4"
				app:layout_gravity="center"
				app:layout_row="0" />

			<View
				android:id="@+id/spaceView1"
				android:layout_width="1dp"
				android:layout_height="4dp"
				android:alpha="1"
				android:background="?attr/colorAccent"
				app:layout_column="0"
				app:layout_columnSpan="5"
				app:layout_row="1" />

			<!-- 2行目 -->
			<TextView
				android:id="@+id/mentuItemText"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/syuntu"
				android:textSize="18sp"
				app:layout_column="0"
				app:layout_gravity="center"
				app:layout_row="2" />

			<Spinner
				android:id="@+id/shunTanYao"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="2"
				app:layout_gravity="center"
				app:layout_row="2" />

			<Spinner
				android:id="@+id/shunYaoThu"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="4"
				app:layout_gravity="center"
				app:layout_row="2" />

			<View
				android:id="@+id/spaceView2"
				android:layout_width="1dp"
				android:layout_height="2dp"
				android:alpha="0.75"
				android:background="?attr/colorAccent"
				app:layout_column="0"
				app:layout_columnSpan="5"
				app:layout_row="3" />

			<!-- 3行目 -->
			<TextView
				android:id="@+id/mentuItemText2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/minkoutu"
				android:textSize="18sp"
				app:layout_column="0"
				app:layout_gravity="center"
				app:layout_row="4" />

			<Spinner
				android:id="@+id/kouMTanYao"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="2"
				app:layout_gravity="center"
				app:layout_row="4" />

			<Spinner
				android:id="@+id/kouMYaoThu"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="4"
				app:layout_gravity="center"
				app:layout_row="4" />

			<View
				android:id="@+id/spaceView3"
				android:layout_width="1dp"
				android:layout_height="2dp"
				android:alpha="0.75"
				android:background="?attr/colorAccent"
				app:layout_column="0"
				app:layout_columnSpan="5"
				app:layout_row="5" />

			<!-- 4行目 -->
			<TextView
				android:id="@+id/mentuItemText3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/ankoutu"
				android:textSize="18sp"
				app:layout_column="0"
				app:layout_gravity="center"
				app:layout_row="6" />

			<Spinner
				android:id="@+id/kouATanYao"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="2"
				app:layout_gravity="center"
				app:layout_row="6" />

			<Spinner
				android:id="@+id/kouAYaoThu"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="4"
				app:layout_gravity="center"
				app:layout_row="6" />

			<View
				android:id="@+id/spaceView4"
				android:layout_width="1dp"
				android:layout_height="2dp"
				android:alpha="0.75"
				android:background="?attr/colorAccent"
				app:layout_column="0"
				app:layout_columnSpan="5"
				app:layout_row="7" />
			<!-- 5行目 -->
			<TextView
				android:id="@+id/mentuItemText4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/minkantu"
				android:textSize="18sp"
				app:layout_column="0"
				app:layout_gravity="center"
				app:layout_row="8" />

			<Spinner
				android:id="@+id/kanMTanYao"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="2"
				app:layout_gravity="center"
				app:layout_row="8" />

			<Spinner
				android:id="@+id/kanMYaoThu"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="4"
				app:layout_gravity="center"
				app:layout_row="8" />

			<View
				android:id="@+id/spaceView5"
				android:layout_width="1dp"
				android:layout_height="2dp"
				android:alpha="0.75"
				android:background="?attr/colorAccent"
				app:layout_column="0"
				app:layout_columnSpan="5"
				app:layout_row="9" />
			<!-- 6行目 -->
			<TextView
				android:id="@+id/mentuItemText5"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:text="@string/ankantu"
				android:textAlignment="center"
				android:textAllCaps="false"

				android:textSize="18sp"
				app:layout_column="0"
				app:layout_gravity="center"
				app:layout_row="10" />

			<Spinner
				android:id="@+id/kanATanYao"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="2"
				app:layout_gravity="center"
				app:layout_row="10" />

			<Spinner
				android:id="@+id/kanAYaoThu"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:entries="@array/list6"
				app:layout_column="4"
				app:layout_gravity="center"
				app:layout_row="10" />

			<View
				android:id="@+id/spaceView6"
				android:layout_width="4dp"
				android:layout_height="1dp"
				android:alpha="1"
				android:background="?attr/colorAccent"
				app:layout_column="1"
				app:layout_row="0"
				app:layout_rowSpan="11" />

			<View
				android:id="@+id/spaceView7"
				android:layout_width="2dp"
				android:layout_height="1dp"
				android:alpha="0.75"
				android:background="?attr/colorAccent"
				app:layout_column="3"
				app:layout_row="0"
				app:layout_rowSpan="11" />
		</android.support.v7.widget.GridLayout>

		<Button
			android:id="@+id/calculateButton"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="44dp"
			android:layout_marginTop="24dp"
			android:layout_marginBottom="16dp"
			android:background="?attr/colorButtonNormal"
			android:text="@string/function"
			app:layout_constraintBottom_toTopOf="@+id/nend2"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/gridLayout" />

		<TextView
			android:id="@+id/markResultText"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="32dp"
			android:text="@string/markResult"
			android:textColor="?attr/colorAccent"
			android:textSize="20sp"
			android:textStyle="bold"
			app:layout_constraintBottom_toBottomOf="@+id/calculateButton"
			app:layout_constraintStart_toEndOf="@+id/calculateButton" />

		<ImageView
			android:id="@+id/imageView3"
			android:layout_width="wrap_content"
			android:layout_height="0dp"
			android:layout_marginStart="8dp"
			android:adjustViewBounds="true"
			app:layout_constraintBottom_toBottomOf="@+id/nend2"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/nend2"
			nendsdk:srcCompat="@drawable/gensou_logo"
			android:contentDescription="@string/logo_image" />

		<net.nend.android.NendAdView
			android:id="@+id/nend2"
			android:layout_width="320dp"
			android:layout_height="100dp"
			android:layout_marginStart="8dp"
			android:layout_marginEnd="8dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			nendsdk:NendAdjustSize="true"
			nendsdk:NendApiKey="@string/nend_ad100"
			nendsdk:NendSpotId="@integer/nend_ad100" />

		<TextView
			android:id="@+id/syoukei1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"

			android:textSize="18sp"
			app:layout_constraintBottom_toBottomOf="@+id/spinner"
			app:layout_constraintStart_toEndOf="@+id/spinner" />

		<TextView
			android:id="@+id/syoukei2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"

			android:textSize="18sp"
			app:layout_constraintBottom_toBottomOf="@+id/spinner2"
			app:layout_constraintStart_toEndOf="@+id/spinner2" />

		<TextView
			android:id="@+id/syoukei3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"

			android:textSize="18sp"
			app:layout_constraintBottom_toBottomOf="@+id/spinner3"
			app:layout_constraintStart_toEndOf="@+id/spinner3" />

		<TextView
			android:id="@+id/syoukei4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"

			android:textSize="18sp"
			app:layout_constraintBottom_toBottomOf="@+id/gridLayout"
			app:layout_constraintStart_toEndOf="@+id/gridLayout" />

		<TextView
			android:id="@+id/syoukei5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="24dp"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toTopOf="@+id/markResultText"
			app:layout_constraintStart_toEndOf="@+id/calculateButton" />

		<TextView
			android:id="@+id/textView11"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:text="@string/mentuDescript"
			app:layout_constraintBottom_toBottomOf="@+id/textView16"
			app:layout_constraintStart_toEndOf="@+id/textView16" />

	</android.support.constraint.ConstraintLayout>
</ScrollView>